# Environment Variables Template
# Copy this to .env and fill in your actual values
# NEVER commit .env to git!

# ===================================
# Google OAuth Configuration
# ===================================
# Get these from: https://console.cloud.google.com/
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# ===================================
# Facebook OAuth Configuration
# ===================================
# Get these from: https://developers.facebook.com/
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id_here
FACEBOOK_APP_SECRET=your_facebook_app_secret_here

# ===================================
# Apple OAuth Configuration
# ===================================
# Get these from: https://developer.apple.com/
REACT_APP_APPLE_CLIENT_ID=your_apple_service_id_here
APPLE_TEAM_ID=your_apple_team_id_here
APPLE_KEY_ID=your_apple_key_id_here
# Note: Apple private key should be stored securely on backend only

# ===================================
# App Configuration
# ===================================
# Your app URL (change for production)
REACT_APP_URL=http://localhost:3000

# API Backend URL (when you create it)
REACT_APP_API_URL=http://localhost:5000

# ===================================
# Backend Only (Node.js/Express)
# ===================================
# JWT secret for token generation
JWT_SECRET=your_secure_random_jwt_secret_here

# Session secret
SESSION_SECRET=your_secure_random_session_secret_here

# Database connection (example)
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Port for backend server
PORT=5000

# Node environment
NODE_ENV=development
